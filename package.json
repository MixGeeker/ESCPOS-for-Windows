{
  "name": "@mixgeeker/node-escpos-win",
  "version": "2.0.0",
  "description": "ESC/POS printer driver for Node.js on Windows, supporting both English and Chinese",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "install": "node-gyp rebuild",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "escpos",
    "printer",
    "windows",
    "pos",
    "thermal-printer",
    "receipt-printer",
    "chinese",
    "gbk",
    "image"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MixGeeker/ESCPOS-for-Windows.git"
  },
  "bugs": {
    "url": "https://github.com/MixGeeker/ESCPOS-for-Windows/issues"
  },
  "homepage": "https://github.com/MixGeeker/ESCPOS-for-Windows#readme",
  "author": "DevTomato <微信：LinkTYLinkBY>",
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.5.0",
    "iconv-lite": "^0.6.3",
    "jimp": "^0.22.12",
    "node-addon-api": "^7.1.0"
  },
  "devDependencies": {
    "node-gyp": "^10.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "gypfile": true,
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts",
    "src/",
    "binding.gyp",
    "README.md",
    "LICENSE"
  ]
}
